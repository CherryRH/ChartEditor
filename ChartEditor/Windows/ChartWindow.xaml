<Window x:Class="ChartEditor.Windows.ChartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:ChartEditor.Windows"
        mc:Ignorable="d"
        Closing="Window_Closing"
        Closed="Window_Closed"
        Title="谱面编辑" Height="900" Width="1600" MinHeight="900" MinWidth="1600">
    <materialDesign:DialogHost
        Identifier="ChartWindowDialog"
        CloseOnClickAway="False">
        <Grid>
            <Frame Name="ChartPage" NavigationUIVisibility="Hidden"/>
        </Grid>
    </materialDesign:DialogHost>
</Window>
